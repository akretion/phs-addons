<?xml version="1.0" encoding="utf-8" ?>
<odoo>
<!-- carrier.account inherit form view -->
<record id="carrier_account_view_form" model="ir.ui.view">
    <field name="name">carrier.account.inherit.view.form</field>
    <field name="model">carrier.account</field>
    <field
            name="inherit_id"
            ref="base_delivery_carrier_label.carrier_account_view_form"
        />
    <field name="arch" type="xml">
        <!-- Add your xpath here -->
        <field name="password" position="after">
            <field name="colisprive_cpcustoid" />
            <field name="colisprive_accountid" />
        </field>
    </field>
</record>
</odoo>
